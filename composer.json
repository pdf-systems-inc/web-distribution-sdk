{
    "name": "pdf-systems-inc/web-distribution-sdk",
    "description": "PHP SDK for Web Distribution",
    "keywords": [
        "pdf-systems-inc",
        "web-distribution-sdk"
    ],
    "homepage": "https://github.com/pdf-systems-inc/web-distribution-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Pungello",
            "email": "rob@pdfsystems.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Pdfsystems\\WebDistributionSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pdfsystems\\WebDistributionSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
